{

  "manifest_version": 2,
  "name": "AwareShopper",
  "version": "1.0",

  "description": "Shop aware on the Internet.",

  "icons": {
    "64": "icons/64x/bag.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["AwareShopper.js"]

    }
  ],

  "browser_action": {
  	"default_icon": "icons/64x/bag-white.png",
	"default_title": "AwareShopper",
	"default_popup": "popup/popup.html"
  },

  "permissions": [
	"tabs"
  ]

}
