{

  "manifest_version": 2,
  "name": "AwareShopper",
  "version": "1.0",

  "description": "Shop aware on the Internet.",

  "icons": {
    "48": "icons/red.png"
  },

  "content_scripts": [
    {
      "matches": ["*://wish.com/*"],
      "js": ["test.js"]

    }
  ],
  
  "browser_action": {
  	"default_icon": "icons/green.png",
	"default_title": "AwareShopper",
	"default_popup": "popup/popup.html"
  }

}
